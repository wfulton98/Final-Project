    "    #Reason for copying is so to not alter the actual df while testing.\n",
    "    copy_df = df.copy()\n",
    "    copy_df[username] = data\n",
    "    \n",
    "    #Fit the df with k=5 (default) to estimate ratings\n",
    "    fit_df = imputer.fit_transform(copy_df)\n",
    "    fit_df = pd.DataFrame(fit_df)\n",
    "    fit_df.rename(columns={ idx: name for idx, name in enumerate(updated_names) }, index={ idx: brewery for idx, brewery in enumerate(brewery_names)}, inplace=True)\n",
